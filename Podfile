platform :ios, '10.0'
use_frameworks!

target 'MySwift' do

#页面布局
pod 'SnapKit', '~> 5.6.0'

#网络请求
pod 'Alamofire','~> 5.6.2'


#APP引导图
pod 'EAIntroView'

#日历
pod 'FSCalendar'

#下拉刷新
pod 'MJRefresh'

#普通轮播图
pod 'SDCycleScrollView'


#图片加载  加载动图
pod 'SDWebImage'

# 下拉框菜单选择
pod 'NewPopMenu'

# 自定义弹窗
pod 'SwiftEntryKit'

#侧边栏
pod 'SideMenu'

#检查网络状态
pod 'ReachabilitySwift'

#很方便的定时器
pod 'SwiftyTimer'

#获取设备信息
pod 'DeviceKit'

#数据保存本地
pod 'SwiftyUserDefaults'

#扫一扫
pod 'swiftScan'


#图片选择包含拍照
pod 'ZLPhotoBrowser'

#图片预览包含文字介绍
pod 'SKPhotoBrowser'

#空列表展示
pod 'EmptyDataSet-Swift'

#键盘弹出
pod 'IQKeyboardManagerSwift'



end